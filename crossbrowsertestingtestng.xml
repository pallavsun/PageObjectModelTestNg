<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Automation Suite" parallel="tests">

	<test name="Chrome Browser  ">
		<parameter name="browser" value="chrome"></parameter>
		<parameter name="url" value="https://nxtgenaiacademy.com/demo-site"></parameter>

		<classes>
			<class name="com.telus.crossbrowsertesting.MultiBrowser"/>
		</classes>

	</test> <!-- Test -->



	<test name="FireFox Browser ">
		<parameter name="browser" value="firefox"></parameter>
		<parameter name="url" value="https://nxtgenaiacademy.com/demo-site"></parameter>

		<classes>
			<class name="com.telus.crossbrowsertesting.MultiBrowser"/>
		</classes>

	</test> <!-- Test -->





</suite> <!-- Automation Suite -->
